{
    "name": "hareom284/laravel-h5p",
    "description": "This is laravel h5p plugin that use for content edition",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.19",
        "h5p/h5p-core": "1.24.*|dev-master",
        "h5p/h5p-editor": "1.24.*|dev-master",
        "guzzlehttp/guzzle": "^7.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hareom284\\LaravelH5p\\": "src/LaravelH5p"
        }
    },
    "authors": [
        {
            "name": "hareom284"
        }
    ],
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2",
        "kitloong/laravel-migrations-generator": "^6.10",
        "laravel/pint": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hareom284\\LaravelH5p\\LaravelH5pServiceProvider"
            ],
            "aliases": {
                "LaravelH5p": "Hareom284\\LaravelH5p\\Facades"
            }
        }
    }
}